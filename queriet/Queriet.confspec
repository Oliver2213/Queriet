[general]
open_on_startup = boolean(default=True)
exit_to_tray = boolean(default=True)
ask_to_exit = boolean(default=True)

[logging]
log_level = option("debug", "info", "warning", "error", "critical", default = "info")
format = string(default="""%(asctime)s - %(name)s - %(levelname)s: %(message)s""")
